<Project>
    <PropertyGroup>
        <LangVersion>latest</LangVersion>

        <Authors>Of Pine Wood</Authors>
        <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
        <PackageLicenseUrl>https://github.com/ofpinewood/pineblog/blob/main/LICENSE</PackageLicense>
        <PackageProjectUrl>https://github.com/ofpinewood/pineblog</PackageProjectUrl>
        <PackageIconUrl>https://github.com/ofpinewood/pineblog/blob/main/pineblog-logo-256x256.gif?raw=true</PackageIcon>
        <RepositoryType>git</RepositoryType>
        <RepositoryUrl>https://github.com/ofpinewood/pineblog</RepositoryUrl>
        <PublishRepositoryUrl>true</PublishRepositoryUrl>
        <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
    </PropertyGroup>
</Project>